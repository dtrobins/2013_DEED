package sluce2.exception;

/**
 * Simple exception thrown when raster layers are not properly loaded into model.
 * 
 * @author Meghan Hutchins
 * @date 09/16/2010
 */
public class RasterLoadException extends Exception
{
    private static final long serialVersionUID = 1L;

    /**
     * 
     */
    public String getMessage()
    {
	return("Unable to load raster data. Check paths to raster files within xml descriptor.");
    }

    /**
     * 
     */
    public String toString()
    {
	return("Unable to load raster data.");
    }
    
}